{
  "multipart": [
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "create_brass_coated:block/brass_girder_encased_shaft/block"
      }
    },
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "create_brass_coated:block/brass_girder_encased_shaft/block",
        "y": 90
      }
    },
    {
      "when": {
        "top": "true"
      },
      "apply": {
        "model": "create_brass_coated:block/brass_girder_encased_shaft/block_top"
      }
    },
    {
      "when": {
        "bottom": "true"
      },
      "apply": {
        "model": "create_brass_coated:block/brass_girder_encased_shaft/block_bottom"
      }
    }
  ]
}